import pygame
from pygame.locals import *

pygame.init()

#Ouverture de la fenÃªtre Pygame
fenetre = pygame.display.set_mode((1500, 117))

#Chargement et collage du fond
fond = pygame.image.load("SCROLL FRAI FINALE - copie.png").convert()
zomby = pygame.image.load("zby.jpeg").convert_alpha() 
fenetre.blit(fond, (0,0))
fenetre.blit(zomby, (0, 0))

pygame.display.flip()

continuer = 1
while continuer:
	continuer = int(input())
