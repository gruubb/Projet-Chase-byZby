
import pygame
from pygame.locals import *

pygame.init()


fenetre = pygame.display.set_mode((9197, 720))

leroux = pygame.image.load("LEROUX REDIM FINAL.png").convert()
fond = pygame.image.load("SCROLL FRAI FINALE redim.png").convert()
zombie = pygame.image.load("MRZBY REDIM FINAL.png").convert()
fenetre.blit(fond, (0,0))
fenetre.blit(zombie, (40,525))
fenetre.blit(leroux, (170,527))

pygame.display.flip()

continuer = 1
while continuer:
	continuer = int(input())
